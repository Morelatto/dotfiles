    <extensionMap>
      <mapping pattern="Dockerfile*" type="Dockerfile" />
      <removed_mapping pattern=".gitignore" approved="true" type="GitIgnore file" />
      <mapping pattern=".env*" type="Env File" />
      <mapping pattern="requirements*.txt" type="Requirements" />
    </extensionMap>
  </component>
</application>