# Git attributes file
# Defines attributes for path names

# Auto detect text files and perform LF normalization
* text=auto

# Force text files to have LF line endings
*.sh text eol=lf
*.zsh text eol=lf
*.bash text eol=lf
*.fish text eol=lf
*.vim text eol=lf
*.conf text eol=lf
*.cfg text eol=lf
*.ini text eol=lf
*.toml text eol=lf
*.yaml text eol=lf
*.yml text eol=lf
*.json text eol=lf
*.md text eol=lf
*.txt text eol=lf
Makefile text eol=lf
makefile text eol=lf

# Programming languages
*.c text diff=cpp
*.cpp text diff=cpp
*.h text diff=cpp
*.hpp text diff=cpp
*.py text diff=python
*.rb text diff=ruby
*.java text diff=java
*.scala text diff=java
*.go text diff=golang
*.js text diff=javascript
*.ts text diff=javascript
*.jsx text diff=javascript
*.tsx text diff=javascript
*.php text diff=php
*.rs text diff=rust
*.swift text diff=swift
*.m text diff=objc

# Documentation
*.md text diff=markdown
*.tex text diff=tex
*.adoc text
*.textile text
*.mustache text
*.csv text
*.tab text
*.tsv text
*.txt text

# Graphics
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.tif binary
*.tiff binary
*.ico binary
*.svg text
*.eps binary

# Fonts
*.ttf binary
*.eot binary
*.otf binary
*.woff binary
*.woff2 binary

# Archives
*.7z binary
*.gz binary
*.tar binary
*.tgz binary
*.zip binary
*.rar binary

# Executables
*.exe binary
*.dll binary
*.so binary
*.dylib binary

# Documents
*.pdf binary
*.doc binary
*.docx binary
*.xls binary
*.xlsx binary
*.ppt binary
*.pptx binary

# Audio/Video
*.mp3 binary
*.mp4 binary
*.avi binary
