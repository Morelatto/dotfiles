{
  "$schema": "https://repomix.com/schemas/latest/schema.json",
  "include": [
    "**/*.py",
    "**/*.js",
    "**/*.ts",
    "**/*.jsx",
    "**/*.tsx",
    "**/*.rs",
    "**/*.go",
    "**/*.java",
    "**/*.cpp",
    "**/*.c",
    "**/*.h",
    "**/*.hpp",
    "**/*.cs",
    "**/*.php",
    "**/*.rb",
    "**/*.swift",
    "**/*.kt",
    "**/*.scala",
    "**/*.sh",
    "**/*.bash",
    "**/*.zsh",
    "**/*.fish",
    "**/*.ps1",
    "**/*.sql",
    "**/*.html",
    "**/*.css",
    "**/*.scss",
    "**/*.sass",
    "**/*.less",
    "**/*.vue",
    "**/*.svelte",
    "**/*.yaml",
    "**/*.yml",
    "**/*.toml",
    "**/*.ini",
    "**/*.conf",
    "**/*.config",
    "**/*.env",
    "**/*.txt",
    "**/*.md",
    "**/*.rst",
    "**/*.dockerfile",
    "**/Dockerfile*",
    "**/docker-compose*",
    "pyproject.toml",
    "requirements*.txt",
    "package.json",
    "package-lock.json",
    "yarn.lock",
    "Cargo.toml",
    "Cargo.lock",
    "go.mod",
    "go.sum",
    "Gemfile*",
    "Pipfile*",
    "poetry.lock",
    "README*",
    "CHANGELOG*",
    "LICENSE*",
    "CONTRIBUTING*",
    "Taskfile.yml",
    "Makefile",
    "**/makefile",
    ".gitignore",
    ".gitattributes",
    ".dockerignore",
    ".editorconfig",
    "tsconfig.json",
    "jsconfig.json",
    "webpack.config.*",
    "vite.config.*",
    "rollup.config.*",
    "babel.config.*",
    "jest.config.*",
    "vitest.config.*",
    "playwright.config.*",
    "cypress.config.*",
    ".eslintrc*",
    ".prettierrc*",
    "tailwind.config.*",
    "next.config.*",
    "nuxt.config.*",
    "vue.config.*"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "**/*.csv",
      "**/*.json",
      "**/*.xlsx",
      "**/*.parquet",
      "**/*.pkl",
      "**/*.bin",
      "**/*.pt",
      "**/*.h5",
      "**/*.hdf5",
      "**/*.ipynb",
      "**/.ipynb_checkpoints/**",
      "**/*.png",
      "**/*.jpg",
      "**/*.jpeg",
      "**/*.gif",
      "**/*.svg",
      "**/*.ico",
      "**/*.pdf",
      "**/*.mp4",
      "**/*.mp3",
      "**/*.wav",
      "**/node_modules/**",
      "**/target/**",
      "**/build/**",
      "**/dist/**",
      "**/.cache/**",
      "**/__pycache__/**",
      "**/.pytest_cache/**",
      "**/.mypy_cache/**",
      "**/.ruff_cache/**",
      "**/venv/**",
      "**/.venv/**",
      "**/env/**",
      "**/.env/**",
      "**/.vscode/**",
      "**/.idea/**",
      "**/*.swp",
      "**/*.swo",
      "**/*~",
      "**/.DS_Store",
      "**/Thumbs.db",
      "**/.git/**",
      "**/.gitignore",
      "**/.gitattributes",
      "**/docs/**/*.md",
      "**/documentation/**",
      "**/*.log",
      "**/logs/**",
      "**/*.bak",
      "**/*.backup",
      "**/*.old"
    ]
  },
  "input": {
    "maxFileSize": 1000000
  },
  "output": {
    "style": "markdown",
    "compress": true,
    "removeComments": true,
    "removeEmptyLines": true,
    "truncateBase64": true,
    "directoryStructure": true,
    "files": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}